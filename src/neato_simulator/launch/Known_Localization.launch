<launch>
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="use_sim_time" value="true"/>
    <arg name="debug" value="false"/>
    <arg name="world_name" value="$(find turtlebot_gazebo)/worlds/playground.world"/>
  </include>

  <node pkg="tf" type="static_transform_publisher" name="laser_to_base" 
    args="0.001453 -0.000453 0.25 0 0 0 base_link base_laser_link 50" />

  <node name="neato_spawn" pkg="gazebo_ros" type="spawn_model" args="-file $(find neato_simulator)/model/model-1_4.sdf -sdf -model create" />



<node name="map_server" pkg="map_server" type="map_server" args="$(find neato_simulator)/maps/map.pgm .05"/>

 <include file="$(find amcl)/examples/amcl_diff.launch" />

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find neato_simulator)/pathPlanning/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find neato_simulator)/pathPlanning/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find neato_simulator)/pathPlanning/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find neato_simulator)/pathPlanning/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find neato_simulator)/pathPlanning/base_local_planner_params.yaml" command="load" />
  </node>

 </launch>
