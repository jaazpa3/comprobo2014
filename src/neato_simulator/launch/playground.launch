<launch>
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="use_sim_time" value="true"/>
    <arg name="debug" value="false"/>
    <arg name="world_name" value="$(find neato_simulator)/worlds/playground.world"/>
  </include>

  <node pkg="tf" type="static_transform_publisher" name="laser_to_base" 
    args="0.001453 -0.000453 0.25 0 0 0 base_link base_laser_link 50" />



  <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher">
    <param name="publish_frequency" type="double" value="30.0" />
  </node>

<node pkg="tf" type="static_transform_publisher" name="map" 
    args="0 0 0 0 0 0 odom map 50" />

 <node name="neato_spawn" pkg="gazebo_ros" type="spawn_model" args="-file $(find neato_simulator)/model/model-1_4.sdf -sdf -model create" /> 




 </launch>
